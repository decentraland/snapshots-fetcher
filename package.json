{
  "name": "@dcl/snapshots-fetcher",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint:check": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint '**/*.{js,ts}' --fix",
    "clean-api-extractor": "rm -rf node_modules/@microsoft/api-extractor/node_modules/typescript || true",
    "api-extractor": "yarn clean-api-extractor && api-extractor run --local --verbose --diagnostics --typescript-compiler-folder ./node_modules/typescript",
    "api-extractor-ci": "yarn clean-api-extractor && api-extractor run --verbose --typescript-compiler-folder ./node_modules/typescript",
    "test": "jest --runInBand --forceExit --detectOpenHandles --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/snapshots-fetcher.git"
  },
  "keywords": [],
  "author": "Decentraland Contributors",
  "license": "Apache-2.0",
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2
  },
  "dependencies": {
    "@dcl/catalyst-storage": "^3.0.1",
    "@dcl/hashing": "^2.0.0",
    "@dcl/schemas": "^8.2.0",
    "@well-known-components/interfaces": "^1.4.0",
    "@well-known-components/metrics": "^2.0.0",
    "p-queue": "^6.6.2"
  },
  "engines": {
    "node": ">16.0.0"
  },
  "engineStrict": true,
  "devDependencies": {
    "@dcl/eslint-config": "^1.1.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "@babel/core": "^7.0.0",
    "@microsoft/api-extractor": "^7.19.4",
    "@types/node": "^18.15.11",
    "@well-known-components/env-config-provider": "^1.1.1",
    "@well-known-components/http-server": "^2.0.0",
    "@well-known-components/logger": "^3.0.0",
    "@well-known-components/test-helpers": "^1.2.1",
    "node-fetch": "^2.6.7",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist"
  ]
}
